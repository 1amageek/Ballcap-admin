{
  "private": false,
  "name": "@1amageek/ballcap-admin",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.4.4",
  "scripts": {
    "lint": "./node_modules/.bin/tslint -p tslint.json",
    "build": "./node_modules/.bin/tsc -d",
    "prepare": "npm run build",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase experimental:functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "description": "Cloud Firestore model framework for TypeScript - Google",
  "contributors": [
    {
      "name": "Norikazu Muramoto",
      "email": "tmy0x3@icloud.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1amageek/ballcap.ts.git"
  },
  "keywords": [
    "firestore",
    "firebase",
    "ballcap",
    "model",
    "typescript"
  ],
  "author": "1amageek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1amageek/ballcap.ts/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@firebase/testing": "^0.15.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "firebase-admin": "^8.7.0",
    "gaxios": "^2.0.1",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "firebase-admin": "^8.7.0"
  },
  "resolutions": {
    "**/gaxios": "1.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
